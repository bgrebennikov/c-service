        <script>
            // AJAX - подгрузка МОДЕЛЕЙ --------------------------------------------------
            $('button.more-models').on('click', function() {

                let count = 0;
                let nextCount = 0;

                $('.model-categories').each(function() {
                    count = $(this).find('.childcat-item').length;
                });
                let id = 18;
                let id_device = 1;
                let url_1 = 'types';
                let url_2 = 'noutbuk';

                let data = { // Сбор данных в один массив
                    'count': count,
                    'id': id,
                    'url_1': url_1,
                    'url_2': url_2,
                    'id_device': id_device
                }



                $.ajax({
                    type: 'POST',
                    url: "/load_more_models",
                    cache: false,
                    data: data,
                    beforeSend: function() {
                        $('button.more-models').text('Загрузка..');
                    },
                    success: function(data) {
                        // console.log(data);
                        $('button.more-models').text('Больше брендов');
                        $('.model-categories').append(data);
                    }
                });


                setTimeout(function() {
                    $('.brand-items').each(function() {
                        nextCount = $(this).find('.brand-item').length;
                        nextCount = nextCount - 16;
                    });
                    if (nextCount < count) {
                        $('button.more-models').css('display', 'none');
                    }
                }, 800);

            });
            // AJAX подгрузка брендов **************************************************
        </script>
